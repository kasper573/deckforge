{
  "name": "deckforge",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"yarn app:dev\" \"yarn api:dev\" --names \"app,api\"",
    "prod": "concurrently \"yarn app:prod\" \"yarn api:prod\" --names \"app,api\"",
    "app:dev": "vite",
    "app:prod": "vite build && vite preview",
    "api:dev": "cross-env NODE_ENV=development tsx watch src/api/start",
    "api:prod": "cross-env NODE_ENV=production tsx src/api/start",
    "build": "yarn typecheck && vite build",
    "typecheck": "tsc --project src/api && tsc --project src/app",
    "postinstall": "prisma generate",
    "lint": "eslint . && prettier . --check",
    "lint:fix": "eslint . --fix && prettier . --write",
    "test:unit": "jest",
    "test:e2e": "cross-var start-server-and-test prod $CYPRESS_BASE_URL 'cypress run'",
    "db:reset": "dotenv-flow \"prisma migrate reset --force\"",
    "db:create-migration": "dotenv-flow prisma migrate dev",
    "db:seed": "dotenv-flow prisma db seed",
    "db:push": "dotenv-flow prisma db push",
    "db:deploy": "dotenv-flow pscale deploy-request create deckforge"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "2.0.0",
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@fontsource/roboto": "4.5.8",
    "@mui/icons-material": "5.10.14",
    "@mui/material": "5.10.14",
    "@prisma/client": "4.6.1",
    "@tanstack/react-query": "4.18.0",
    "@trpc/client": "10.1.0",
    "@trpc/react-query": "10.1.0",
    "@trpc/server": "10.1.0",
    "@vercel/analytics": "0.1.5",
    "cors": "2.8.5",
    "express": "4.18.2",
    "express-jwt": "7.7.7",
    "history": "4.10.1",
    "immer": "9.0.16",
    "jwks-rsa": "3.0.0",
    "morgan": "1.10.0",
    "path-to-regexp": "6.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router": "5.3.3",
    "react-router-dom": "5.3.3",
    "react-typesafe-routes": "0.0.8",
    "superjson": "1.11.0",
    "use-debounce": "8.0.4",
    "uuid": "9.0.0",
    "zod": "3.19.1",
    "zustand": "^4.1.4"
  },
  "devDependencies": {
    "@swc-node/jest": "1.5.5",
    "@swc/core": "1.3.18",
    "@testing-library/cypress": "8.0.7",
    "@types/cors": "2.8.12",
    "@types/dotenv-flow": "3.2.0",
    "@types/express": "4.17.14",
    "@types/jest": "29.2.3",
    "@types/morgan": "1.9.3",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "@vitejs/plugin-react": "2.2.0",
    "concurrently": "7.6.0",
    "cross-env": "7.0.3",
    "cross-var": "1.1.0",
    "cypress": "11.2.0",
    "cypress-dotenv-flow": "1.2.2",
    "dotenv-flow": "3.2.0",
    "dotenv-flow-cli": "^1.0.0",
    "eslint": "8.27.0",
    "eslint-config-next": "13.0.3",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-mui-path-imports": "0.0.3",
    "eslint-plugin-unused-imports": "2.0.0",
    "jest": "29.3.1",
    "prettier": "2.7.1",
    "prettier-plugin-prisma": "4.4.0",
    "prisma": "4.6.1",
    "start-server-and-test": "1.14.0",
    "tsx": "3.12.1",
    "typescript": "4.9.3",
    "vite": "3.2.3",
    "vite-plugin-checker": "0.5.1",
    "zod-prisma": "0.5.4"
  }
}
